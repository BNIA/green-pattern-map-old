            div#sites
                ul.nav.navbar-nav
                    li.dropdown.active.span8
                        a.dropdown-toggle(href='#', data-toggle='dropdown', role='button')
                            span#site-select-title Sites
                            span.caret
                        ul#site-select.dropdown-menu(role='menu')
                            li
                                .radio
                                    label
                                        input(type='radio', name='site_options')
                                        | Community Managed Open Spaces
                            li
                                .radio
                                    label
                                        input(type='radio', name='site_options')
                                        | Stormwater Management
            div#layers
                ul.nav.navbar-nav
                    li.dropdown.active.span8
