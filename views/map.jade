                    input#search-sitename.search-switch.typeahead.form-control(type='text', placeholder='Search Site Names...')
                    input#search-csa.search-switch.typeahead.form-control(type='text', placeholder='Search CSAs...')
                    input#search-neighborhood.search-switch.typeahead.form-control(type='text', placeholder='Search Neighborhoods...')
