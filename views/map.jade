                            span#site-select-title Options
                            span.caret
                        ul#site-select.dropdown-menu(role='menu')
                            li
                                .checkbox
                                    label
                                        input(type='checkbox', name='global_options')
                                        | Display Sites Outside Layer Selection
                            li
                                .checkbox
                                    label
                                        input(type='checkbox', name='global_options')
                                        | More Stuff
            ul.nav.navbar-nav.navbar-right
                li
                    a(href='#') Link

    div#config
